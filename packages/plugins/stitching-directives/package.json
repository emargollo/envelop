{
  "name": "@envelop/stitching-directives",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/n1ru4l/envelop.git",
    "directory": "packages/plugins/stitching-directives"
  },
  "author": "Emmanuel Argollo <emmanuel.argollo@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "scripts": {
    "test": "jest"
  },
  "peerDependencies": {
    "@envelop/core": "^4.0.0",
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-tools/schema": "10.0.0",
    "@graphql-tools/stitching-directives": "^3.0.0",
    "@graphql-tools/utils": "^10.0.1"
  },
  "devDependencies": {
    "@envelop/core": "^4.0.0",
    "graphql": "16.6.0",
    "typescript": "5.1.3"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
